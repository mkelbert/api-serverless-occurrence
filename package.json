{
  "name": "api-serverless-occurrence",
  "version": "1.0.0",
  "description": "Api serverless to save occurrence",
  "main": "index.js",
  "scripts": {
    "build": "babel src -d dist/",
    "build:dev": "babel --watch src -d dist/",
    "deploy": "npm run build && sls deploy",
    "offline": "sls offline start --migrate",
    "sls:remove": "sls remove",
    "test": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register",
    "test:tdd": "./node_modules/.bin/mocha tests/**/*.spec.js --require babel-register --watch",
    "test:coverage": "nyc npm test",
    "test:tdd:db": "./node_modules/.bin/mocha tests/models/DB.spec.js --require babel-register --watch",
    "test:tdd:user": "./node_modules/.bin/mocha tests/models/User.spec.js --require babel-register --watch",
    "lint": "./node_modules/.bin/eslint src/**/*.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kelvinoenning/api-serverless-occurrence.git"
  },
  "author": "Kelvin Oenning",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kelvinoenning/api-serverless-occurrence/issues"
  },
  "homepage": "https://github.com/kelvinoenning/api-serverless-occurrence#readme",
  "devDependencies": {
    "aws-sdk": "^2.122.0",
    "babel-cli": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-preset-babili": "^0.1.4",
    "babel-preset-env": "^1.3.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.24.0",
    "babili": "^0.1.4",
    "chai": "^3.5.0",
    "eslint": "^4.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-plugin-import": "^2.6.1",
    "husky": "^0.11.9",
    "mocha": "^3.2.0",
    "nyc": "^10.2.0",
    "rimraf": "^2.6.1",
    "serverless": "^1.23.0",
    "serverless-dynamodb-client": "0.0.2",
    "serverless-dynamodb-local": "^0.2.25",
    "serverless-offline": "^3.16.0",
    "sinon": "^4.0.0",
    "sinon-chai": "^2.14.0",
    "sinon-stub-promise": "^4.0.0"
  },
  "nyc": {
    "reporter": [
      "text",
      "html"
    ],
    "exclude": [
      "tests/**"
    ]
  },
  "dependencies": {
    "node-fetch": "^1.7.3"
  }
}
